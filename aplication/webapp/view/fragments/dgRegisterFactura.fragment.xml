<core:FragmentDefinition
xmlns="sap.m"
xmlns:core="sap.ui.core"
xmlns:mvc="sap.ui.core.mvc"   
xmlns:l="sap.ui.layout"
xmlns:f="sap.ui.layout.form"
>
<Dialog contentWidth="30rem" title="Registro de factura" icon="sap-icon://sales-order" titleAlignment="Center">
    <VBox justifyContent="Center" alignItems="Center" class="sapUiSmallMargin">
        <Text text="¿De qué forma quiere realizar dicho procedimiento?" class="sapUiTinyMargin"/>
        <Button text="Carga de factura electrónica" press="dgRegisterFactura_electronica" icon="sap-icon://upload" type="Emphasized" />
        <Button text="Carga de factura física" press="dgRegisterFactura_fisica" icon="sap-icon://keyboard-and-mouse" type="Emphasized"/>
    </VBox>
    <OverflowToolbar>
        <ToolbarSpacer/>
        <Button text="Cancelar" press="dgCancelRegisterFactura"/>
    </OverflowToolbar>
</Dialog>
</core:FragmentDefinition>
